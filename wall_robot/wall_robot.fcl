FUNCTION_BLOCK robot_wall

VAR_INPUT
rd : REAL;
dq : REAL;
o : REAL;
v : REAL;
END_VAR

VAR_OUTPUT
la : REAL;
av : REAL;
END_VAR

FUZZIFY rd
TERM L := (0, 0) (0, 1) (1, 0);
TERM M := (0, 0) (1, 1) (2, 0);
TERM H := (1, 0) (2, 1) (3, 0);
TERM VH := (2, 0) (3, 1) (3, 0);
RANGE := (0..3);
END_FUZZIFY

FUZZIFY dq
TERM L := (0, 0) (0, 1) (2, 0);
TERM H := (0, 0) (2, 1) (2, 0);
RANGE := (0..2);
END_FUZZIFY

FUZZIFY o
TERM HL := (-450, 0) (-450, 1) (-225, 0);
TERM LL := (-450, 0) (-225, 1) (0, 0);
TERM Z := (-225, 0) (0, 1) (225, 0);
TERM LR := (0, 0) (225, 1) (450, 0);
TERM HR := (225, 0) (450, 1) (450, 0);
RANGE := (-450..450);
END_FUZZIFY

FUZZIFY v
TERM L := (0, 0) (0, 1) (1, 0);
TERM H := (0, 0) (1, 1) (1, 0);
RANGE := (0..1);
END_FUZZIFY

DEFUZZIFY la
TERM VHB := (-1, 0) (-1, 1) (-0.75, 0);
TERM HB := (-1, 0) (-0.75, 1) (-0.5, 0);
TERM MB := (-0.75, 0) (-0.5, 1) (-0.25, 0);
TERM SB := (-0.5, 0) (-0.25, 1) (0, 0);
TERM Z := (-0.25, 0) (0, 1) (0.25, 0);
TERM SA := (0, 0) (0.25, 1) (0.5, 0);
TERM MA := (0.25, 0) (0.5, 1) (0.75, 0);
TERM HA := (0.5, 0) (0.75, 1) (1, 0);
TERM VHA := (0.75, 0) (1, 1) (1, 0);
METHOD : COG;
RANGE := (-1..1);
END_DEFUZZIFY

DEFUZZIFY av
TERM VHR := (-1, 0) (-1, 1) (-0.75, 0);
TERM HR := (-1, 0) (-0.75, 1) (-0.5, 0);
TERM MR := (-0.75, 0) (-0.5, 1) (-0.25, 0);
TERM SR := (-0.5, 0) (-0.25, 1) (0, 0);
TERM Z := (-0.25, 0) (0, 1) (0.25, 0);
TERM SL := (0, 0) (0.25, 1) (0.5, 0);
TERM ML := (0.25, 0) (0.5, 1) (0.75, 0);
TERM HL := (0.5, 0) (0.75, 1) (1, 0);
TERM VHL := (0.75, 0) (1, 1) (1, 0);
METHOD : COG;
RANGE := (-1..1);
END_DEFUZZIFY

RULEBLOCK rules
AND : MIN;
RULE 01: IF rd IS L AND dq IS L AND o IS LL AND v IS L THEN la IS VHB , av IS VHR WITH 0.4610;
RULE 02: IF rd IS L AND dq IS L AND o IS LL AND v IS H THEN la IS VHB , av IS VHR WITH 0.4896;
